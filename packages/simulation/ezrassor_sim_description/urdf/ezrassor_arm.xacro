<?xml version='1.0'?>

<robot name="ezrassor" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <xacro:property name="cameraSize" value="0.01"/>
  <xacro:property name="cameraMass" value="0.1"/>

  <xacro:include filename="$(find ezrassor_sim_description)/urdf/ezrassor.gazebo" />
  <xacro:include filename="$(find ezrassor_sim_description)/urdf/materials.xacro" />
  <xacro:include filename="$(find ezrassor_sim_description)/urdf/macros.xacro" />

  <!--Below are all of the robot's Links/-->
  <link name='base_link'>
   <pose>0 0 0 0 0 0</pose>
  </link>

  <link name='body'>
    <pose>0 0 0 0 0 0</pose>
    <inertial>
      <mass value="15.0"/>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="0.5" ixy="0" ixz="0"
          iyy="1.0" iyz="0"
          izz="0.1"
      />
    </inertial>
    <collision name='collision'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <base_unit/>
      </geometry>
    </collision>
    <visual name='base_link_visual'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <base_unit/>
      </geometry>
    </visual>
  </link>

  <link name="left_wheel_front">
    <collision name="collision">
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <robot_wheel />
      </geometry>
      <surface>
        <friction>
          <mu> .6 </mu>
          <mu2> .6 </mu2>
        </friction>
      </surface>
      <surface>
        <friction>
          <mu> .6 </mu>
          <mu2> .6 </mu2>
        </friction>
      </surface>
    </collision>
    <visual name="left_wheel_front_visual">
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <robot_wheel />
      </geometry>
    </visual>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <mass value="5"/>
      <cylinder_inertia m="5" r="0.18" h="0.2"/>
    </inertial>
  </link>

  <link name="right_wheel_front">
    <collision name="collision">
      <origin xyz="0 0 0" rpy="0 0 3.14159"/>
      <geometry>
        <robot_wheel />
      </geometry>
      <surface>
        <friction>
        <mu> .6 </mu>
        <mu2> .6 </mu2>
        </friction>
      </surface>
    </collision>
    <visual name="right_wheel_front_visual">
      <origin xyz="0 0 0" rpy="0 0 3.14159"/>
      <geometry>
        <robot_wheel />
      </geometry>
    </visual>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 3.14159"/>
      <mass value="5"/>
      <cylinder_inertia m="5" r="0.18" h="0.2"/>
    </inertial>
  </link>

  <link name="left_wheel_back">
    <collision name="collision">
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <robot_wheel />
      </geometry>
        <surface>
          <friction>
            <mu> .6 </mu>
            <mu2> .6 </mu2>
         </friction>
        </surface>
    </collision>
    <visual name="left_wheel_back_visual">
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <robot_wheel />
      </geometry>
    </visual>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="5"/>
      <cylinder_inertia m="5" r="0.18" h="0.2"/>
    </inertial>
  </link>

  <link name="right_wheel_back">
    <collision name="collision">
      <origin xyz="0 0 0" rpy="0 0 3.14159"/>
      <geometry>
        <robot_wheel />
      </geometry>
      <surface>
        <friction>
          <mu> .6 </mu>
          <mu2> .6 </mu2>
        </friction>
      </surface>
    </collision>
    <visual name="right_wheel_back_visual">
      <origin xyz="0 0 0" rpy="0 0 3.14159"/>
      <geometry>
        <robot_wheel />
      </geometry>
    </visual>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 3.14159"/>
      <mass value="5"/>
      <cylinder_inertia m="5" r="0.18" h="0.2"/>
    </inertial>
  </link>

  <link name="arm_back">
    <collision name="collision">
      <origin xyz="0 0 0" rpy="3.1415 0 3.1415"/>
      <geometry>
        <drum_arm />
      </geometry>
    </collision>
    <visual name="visual">
      <origin xyz="0 0 0" rpy="3.1415 0 3.1415"/>
      <geometry>
        <drum_arm />
      </geometry>
    </visual>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 3.1415"/>
      <mass value="5"/>
      <cylinder_inertia m="5" r="0.05" h="0.3"/>
      <inertia
        ixx="1.0" ixy="0.0" ixz="0.0"
        iyy="1.0" iyz="0.0"
        izz="1.0"/>
    </inertial>
  </link>


  <link name="drum_back">
    <!-- <collision name="collision">
      <origin xyz="0 0 0" rpy="0 0 3.14159"/>
      <geometry>
        <robot_arm_drum />
      </geometry>
    </collision> -->
     <collision name="collision">
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.1839" length="1.0"/>
      </geometry>
    </collision>
    <visual name="drum_back">
      <origin xyz="0 0 0" rpy="3.1415 0 3.14159"/>
      <geometry>
        <robot_arm_drum />
      </geometry>
    </visual>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 3.14159"/>
      <mass value="5"/>
      <cylinder_inertia m="5" r="0.1839" h="1.0"/>
    </inertial>
  </link>


