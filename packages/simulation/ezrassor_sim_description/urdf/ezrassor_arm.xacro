      </contact_coefficients>
      <surface>
        <bounce>
          <restitution_coefficient>0</restitution_coefficient>
        </bounce>
        <friction>
          <ode>
            <fdir1>0 0 0</fdir1>
            <mu>1</mu>
            <mu2>1</mu2>
            <slip1>0</slip1>
            <slip2>0</slip2>
